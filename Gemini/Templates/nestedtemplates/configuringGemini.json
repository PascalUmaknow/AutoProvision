{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "spappVMName": {
      "type": "string"
    },
    "sqlVMName": {
      "type": "string"
    },
    "location": {
      "type": "string"
    },
    "spConfigurationModulesURL": {
      "type": "string"
    },
    "spConfigurationFunction": {
      "type": "string"
    },
    "domainName": {
      "type": "string"
    },
    "adminUserName": {
      "type": "string"
    },
    "adminPassword": {
      "type": "securestring"
    },
    "GeminiSetupUserAccountUserName": {
      "type": "string"
    },
    "GeminiFarmAccountUserName": {
      "type": "string"
    },
    "sqlServerServiceAccountUserName": {
      "type": "string"
    },
    "sqlAOAGName": {
      "type": "string"
    },
    "administrationContentDatabaseName": {
      "type": "string"
    },
    "contentDatabaseName": {
      "type": "string"
    },
    "configDatabaseName": {
      "type": "string"
    },
    "GeminiCAfqdn": {
      "type": "string"
    },
    "GeminiSetupUserAccountPassword": {
      "type": "securestring"
    },
    "GeminiFarmAccountPassword": {
      "type": "securestring"
    },
    "GeminiFarmPassphrasePassword": {
      "type": "securestring"
    },
    "sqlServerServiceAccountPassword": {
      "type": "securestring"
    },
    "spwebVMName": {
      "type": "string"
    },
    "GeminiFarmName": {
      "type": "string"
    },
    "GeminiWebfqdn": {
      "type": "string"
    },
    "spSiteTemplateName": {
      "type": "string"
    }
  },
  "resources": [
    {
      "type": "Microsoft.Compute/virtualMachines/extensions",
      "name": "[concat(parameters('spappVMName'),'0/ConfigureSPAppServer')]",
      "apiVersion": "2015-06-15",
      "location": "[parameters('location')]",
      "properties": {
        "publisher": "Microsoft.Powershell",
        "type": "DSC",
        "typeHandlerVersion": "2.20",
        "autoUpgradeMinorVersion": true,
        "settings": {
          "modulesURL": "[parameters('spConfigurationModulesURL')]",
          "wmfVersion": "4.0",
          "configurationFunction": "[parameters('spConfigurationFunction')]",
          "properties": {
            "domainName": "[parameters('domainName')]",
            "adminCreds": {
              "userName": "[parameters('adminUserName')]",
              "password": "privateSettingsRef:adminPassword"
            },
            "GeminiSetupUserAccountcreds": {
              "userName": "[parameters('GeminiSetupUserAccountUserName')]",
              "password": "privateSettingsRef:GeminiSetupUserAccountPassword"
            },
            "GeminiFarmAccountcreds": {
              "userName": "[parameters('GeminiFarmAccountUserName')]",
              "password": "privateSettingsRef:GeminiFarmAccountPassword"
            },
            "GeminiFarmPassphrasecreds": {
              "userName": "ignore",
              "password": "privateSettingsRef:GeminiFarmPassphrasePassword"
            },
            "sqlServiceCreds": {
              "userName": "[parameters('sqlServerServiceAccountUserName')]",
              "password": "privateSettingsRef:sqlServerServiceAccountPassword"
            },
            "databaseName": "[parameters('configDatabaseName')]",
            "sqlAlwaysOnAvailabilityGroupName": "[parameters('sqlAOAGName')]",
            "primaryReplica": "[concat(parameters('sqlVMName'),'1')]",
            "secondaryReplica": "[concat(parameters('sqlVMName'),'0')]",
            "databaseNames": [
              "[parameters('administrationContentDatabaseName')]",
              "[parameters('configDatabaseName')]"
            ],
            "administrationContentDatabaseName": "[parameters('administrationContentDatabaseName')]",
            "databaseServer": "[concat(parameters('sqlVMName'),'1.',parameters('domainName'))]",
            "configuration": "[concat('{\"roles\":[{\"type\":\"application\",\"properties\":{\"fqdn\":\"',parameters('GeminiCAfqdn'),'\",\"port\":80}}],\"configureForHa\":\"True\",\"loadBalancedSetProbePort\":8088}')]"
          }
        },
        "protectedSettings": {
          "items": {
            "adminPassword": "[parameters('adminPassword')]",
            "GeminiSetupUserAccountPassword": "[parameters('GeminiSetupUserAccountPassword')]",
            "GeminiFarmAccountPassword": "[parameters('GeminiFarmAccountPassword')]",
            "GeminiFarmPassphrasePassword": "[parameters('GeminiFarmPassphrasePassword')]",
            "sqlServerServiceAccountPassword": "[parameters('sqlServerServiceAccountPassword')]"
          }
        }
      }
    },
    {
      "type": "Microsoft.Compute/virtualMachines/extensions",
      "name": "[concat(parameters('spappVMName'),'1/ConfigureSPAppServer')]",
      "apiVersion": "2015-06-15",
      "location": "[parameters('location')]",
      "dependsOn": [
        "[resourceId('Microsoft.Compute/virtualMachines/extensions', concat(parameters('spappVMName'),'0'),'ConfigureSPAppServer')]"
      ],
      "properties": {
        "publisher": "Microsoft.Powershell",
        "type": "DSC",
        "typeHandlerVersion": "2.20",
        "autoUpgradeMinorVersion": true,
        "settings": {
          "modulesURL": "[parameters('spConfigurationModulesURL')]",
          "wmfVersion": "4.0",
          "configurationFunction": "[parameters('spConfigurationFunction')]",
          "properties": {
            "domainName": "[parameters('domainName')]",
            "adminCreds": {
              "userName": "[parameters('adminUserName')]",
              "password": "privateSettingsRef:adminPassword"
            },
            "GeminiSetupUserAccountcreds": {
              "userName": "[parameters('GeminiSetupUserAccountUserName')]",
              "password": "privateSettingsRef:GeminiSetupUserAccountPassword"
            },
            "GeminiFarmAccountcreds": {
              "userName": "[parameters('GeminiFarmAccountUserName')]",
              "password": "privateSettingsRef:GeminiFarmAccountPassword"
            },
            "GeminiFarmPassphrasecreds": {
              "userName": "ignore",
              "password": "privateSettingsRef:GeminiFarmPassphrasePassword"
            },
            "sqlServiceCreds": {
              "userName": "[parameters('sqlServerServiceAccountUserName')]",
              "password": "privateSettingsRef:sqlServerServiceAccountPassword"
            },
            "databaseName": "[parameters('configDatabaseName')]",
            "administrationContentDatabaseName": "[parameters('administrationContentDatabaseName')]",
            "databaseServer": "[concat(parameters('sqlVMName'),'1.',parameters('domainName'))]",
            "sqlAlwaysOnAvailabilityGroupName": "[parameters('sqlAOAGName')]",
            "primaryReplica": "[concat(parameters('sqlVMName'),'1')]",
            "secondaryReplica": "[concat(parameters('sqlVMName'),'0')]",
            "databaseNames": null,
            "configuration": "[concat('{\"roles\":[{\"type\":\"application\",\"properties\":{\"fqdn\":\"',parameters('GeminiCAfqdn'),'\",\"port\":80}}],\"configureForHa\":\"True\",\"loadBalancedSetProbePort\":8088}')]"
          }
        },
        "protectedSettings": {
          "items": {
            "adminPassword": "[parameters('adminPassword')]",
            "GeminiSetupUserAccountPassword": "[parameters('GeminiSetupUserAccountPassword')]",
            "GeminiFarmAccountPassword": "[parameters('GeminiFarmAccountPassword')]",
            "GeminiFarmPassphrasePassword": "[parameters('GeminiFarmPassphrasePassword')]",
            "sqlServerServiceAccountPassword": "[parameters('sqlServerServiceAccountPassword')]"
          }
        }
      }
    },
    {
      "type": "Microsoft.Compute/virtualMachines/extensions",
      "name": "[concat(parameters('spwebVMName'),'0/ConfigureSPWebServer')]",
      "apiVersion": "2015-06-15",
      "location": "[parameters('location')]",
      "dependsOn": [
        "[resourceId('Microsoft.Compute/virtualMachines/extensions', concat(parameters('spappVMName'),'0'), 'ConfigureSPAppServer')]"
      ],
      "properties": {
        "publisher": "Microsoft.Powershell",
        "type": "DSC",
        "typeHandlerVersion": "2.20",
        "autoUpgradeMinorVersion": true,
        "settings": {
          "modulesURL": "[parameters('spConfigurationModulesURL')]",
          "wmfVersion": "4.0",
          "configurationFunction": "[parameters('spConfigurationFunction')]",
          "properties": {
            "domainName": "[parameters('domainName')]",
            "adminCreds": {
              "userName": "[parameters('adminUserName')]",
              "password": "privateSettingsRef:adminPassword"
            },
            "GeminiSetupUserAccountcreds": {
              "userName": "[parameters('GeminiSetupUserAccountUserName')]",
              "password": "privateSettingsRef:GeminiSetupUserAccountPassword"
            },
            "GeminiFarmAccountcreds": {
              "userName": "[parameters('GeminiFarmAccountUserName')]",
              "password": "privateSettingsRef:GeminiFarmAccountPassword"
            },
            "GeminiFarmPassphrasecreds": {
              "userName": "ignore",
              "password": "privateSettingsRef:GeminiFarmPassphrasePassword"
            },
            "sqlServiceCreds": {
              "userName": "[parameters('sqlServerServiceAccountUserName')]",
              "password": "privateSettingsRef:sqlServerServiceAccountPassword"
            },
            "databaseName": "[parameters('configDatabaseName')]",
            "administrationContentDatabaseName": "[parameters('administrationContentDatabaseName')]",
            "sqlAlwaysOnAvailabilityGroupName": "[parameters('sqlAOAGName')]",
            "primaryReplica": "[concat(parameters('sqlVMName'),'1')]",
            "secondaryReplica": "[concat(parameters('sqlVMName'),'0')]",
            "databaseNames": "[parameters('contentDatabaseName')]",
            "databaseServer": "[concat(parameters('sqlVMName'),'1.',parameters('domainName'))]",
            "configuration": "[concat('{\"roles\":[{\"type\":\"web\",\"properties\":{\"webApp\":{\"name\":\"',parameters('GeminiFarmName'),'\",\"applicationPool\":\"',parameters('GeminiFarmName'),' AppPool\",\"applicationPoolAccount\":\"', parameters('GeminiFarmAccountUserName'),'\",\"url\":\"http://',parameters('GeminiWebfqdn'),'\",\"port\":80,\"hostHeader\":\"',parameters('GeminiWebfqdn'),'\",\"databaseName\":\"',parameters('contentDatabaseName'),'\"},\"site\":{\"name\":\"',parameters('GeminiFarmName'),'\",\"template\":\"',parameters('spSiteTemplateName'),'\",\"url\":\"http://',parameters('GeminiWebfqdn'),'\",\"ownerAliasDomain\":\"',parameters('domainName'),'\",\"ownerAliasUserName\":\"',parameters('GeminiFarmAccountUserName'),'\",\"secondaryOwnerAliasDomain\":\"',parameters('domainName'),'\",\"secondaryOwnerAliasUserName\":\"',parameters('adminUserName'),'\"}}}],\"configureForHa\":\"True\",\"loadBalancedSetProbePort\":8088}')]"
          }
        },
        "protectedSettings": {
          "items": {
            "adminPassword": "[parameters('adminPassword')]",
            "GeminiSetupUserAccountPassword": "[parameters('GeminiSetupUserAccountPassword')]",
            "GeminiFarmAccountPassword": "[parameters('GeminiFarmAccountPassword')]",
            "GeminiFarmPassphrasePassword": "[parameters('GeminiFarmPassphrasePassword')]",
            "sqlServerServiceAccountPassword": "[parameters('sqlServerServiceAccountPassword')]"
          }
        }
      }
    },
    {
      "type": "Microsoft.Compute/virtualMachines/extensions",
      "name": "[concat(parameters('spwebVMName'),'1/ConfigureSPWebServer')]",
      "apiVersion": "2015-06-15",
      "location": "[parameters('location')]",
      "dependsOn": [
        "[resourceId('Microsoft.Compute/virtualMachines/extensions', concat(parameters('spwebVMName'),'0'), 'ConfigureSPWebServer')]"
      ],
      "properties": {
        "publisher": "Microsoft.Powershell",
        "type": "DSC",
        "typeHandlerVersion": "2.20",
        "autoUpgradeMinorVersion": true,
        "settings": {
          "modulesURL": "[parameters('spConfigurationModulesURL')]",
          "wmfVersion": "4.0",
          "configurationFunction": "[parameters('spConfigurationFunction')]",
          "properties": {
            "domainName": "[parameters('domainName')]",
            "adminCreds": {
              "userName": "[parameters('adminUserName')]",
              "password": "privateSettingsRef:adminPassword"
            },
            "GeminiSetupUserAccountcreds": {
              "userName": "[parameters('GeminiSetupUserAccountUserName')]",
              "password": "privateSettingsRef:GeminiSetupUserAccountPassword"
            },
            "GeminiFarmAccountcreds": {
              "userName": "[parameters('GeminiFarmAccountUserName')]",
              "password": "privateSettingsRef:GeminiFarmAccountPassword"
            },
            "GeminiFarmPassphrasecreds": {
              "userName": "ignore",
              "password": "privateSettingsRef:GeminiFarmPassphrasePassword"
            },
            "sqlServiceCreds": {
              "userName": "[parameters('sqlServerServiceAccountUserName')]",
              "password": "privateSettingsRef:sqlServerServiceAccountPassword"
            },
            "databaseName": "[parameters('configDatabaseName')]",
            "administrationContentDatabaseName": "[parameters('administrationContentDatabaseName')]",
            "sqlAlwaysOnAvailabilityGroupName": "[parameters('sqlAOAGName')]",
            "primaryReplica": "[concat(parameters('sqlVMName'),'1')]",
            "secondaryReplica": "[concat(parameters('sqlVMName'),'0')]",
            "databaseNames": null,
            "databaseServer": "[concat(parameters('sqlVMName'),'1.',parameters('domainName'))]",
            "configuration": "[concat('{\"roles\":[{\"type\":\"web\",\"properties\":{\"webApp\":{\"name\":\"',parameters('GeminiFarmName'),'\",\"applicationPool\":\"',parameters('GeminiFarmName'),' AppPool\",\"applicationPoolAccount\":\"', parameters('GeminiFarmAccountUserName'),'\",\"url\":\"http://',parameters('GeminiWebfqdn'),'\",\"port\":80,\"hostHeader\":\"',parameters('GeminiWebfqdn'),'\",\"databaseName\":\"',parameters('contentDatabaseName'),'\"},\"site\":{\"name\":\"',parameters('GeminiFarmName'),'\",\"template\":\"',parameters('spSiteTemplateName'),'\",\"url\":\"http://',parameters('GeminiWebfqdn'),'\",\"ownerAliasDomain\":\"',parameters('domainName'),'\",\"ownerAliasUserName\":\"',parameters('GeminiFarmAccountUserName'),'\",\"secondaryOwnerAliasDomain\":\"',parameters('domainName'),'\",\"secondaryOwnerAliasUserName\":\"',parameters('adminUserName'),'\"}}}],\"configureForHa\":\"True\",\"loadBalancedSetProbePort\":8088}')]"
          }
        },
        "protectedSettings": {
          "items": {
            "adminPassword": "[parameters('adminPassword')]",
            "GeminiSetupUserAccountPassword": "[parameters('GeminiSetupUserAccountPassword')]",
            "GeminiFarmAccountPassword": "[parameters('GeminiFarmAccountPassword')]",
            "GeminiFarmPassphrasePassword": "[parameters('GeminiFarmPassphrasePassword')]",
            "sqlServerServiceAccountPassword": "[parameters('sqlServerServiceAccountPassword')]"
          }
        }
      }
    }
  ],
  "outputs": {}
}