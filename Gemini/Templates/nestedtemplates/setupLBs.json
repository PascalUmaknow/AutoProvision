{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "type": "string"
    },
    "geminiCALBName": {
      "type": "string"
    },
    "geminiCALBFE": {
      "type": "string"
    },
    "geminiCAResourceId": {
      "type": "string"
    },
    "geminiCALBBE": {
      "type": "string"
    },
    "geminiCANAT": {
      "type": "string"
    },
    "geminiCALBFEConfigID": {
      "type": "string"
    },
    "geminiWebLBName": {
      "type": "string"
    },
    "geminiWebLBFE": {
      "type": "string"
    },
    "spWebResourceId": {
      "type": "string"
    },
    "geminiWebLBBE": {
      "type": "string"
    },
    "geminiWebLB": {
      "type": "string"
    },
    "geminiWebLBFEConfigID": {
      "type": "string"
    },
    "geminiWebBEAddressPoolID": {
      "type": "string"
    },
    "geminiWebProbeID": {
      "type": "string"
    },
    "gemininiWebProbe": {
      "type": "string"
    },
    "sqlLBName": {
      "type": "string"
    },
    "sqlLBFE": {
      "type": "string"
    },
    "sqlLBIPAddress": {
      "type": "string"
    },
    "staticSubnetRef": {
      "type": "string"
    },
    "sqlLBBE": {
      "type": "string"
    },
    "sqllbFEConfigID": {
      "type": "string"
    },
    "sqllbProbeID": {
      "type": "string"
    },
    "SQLAOProbe": {
      "type": "string"
    }
  },
  "resources": [
    {
      "apiVersion": "2015-06-15",
      "name": "[parameters('geminiCALBName')]",
      "type": "Microsoft.Network/loadBalancers",
      "location": "[parameters('location')]",
      "properties": {
        "frontendIPConfigurations": [
          {
            "name": "[parameters('geminiCALBFE')]",
            "properties": {
              "publicIPAddress": {
                "id": "[parameters('geminiCAResourceId')]"
              }
            }
          }
        ],
        "backendAddressPools": [
          {
            "name": "[parameters('geminiCALBBE')]"
          }
        ],
        "inboundNatRules": [
          {
            "name": "[parameters('geminiCANAT')]",
            "properties": {
              "frontendIPConfiguration": {
                "id": "[parameters('geminiCALBFEConfigID')]"
              },
              "protocol": "tcp",
              "frontendPort": 80,
              "backendPort": 80,
              "enableFloatingIP": false
            }
          }
        ]
      }
    },
    {
      "apiVersion": "2015-06-15",
      "name": "[parameters('geminiWebLBName')]",
      "type": "Microsoft.Network/loadBalancers",
      "location": "[parameters('location')]",
      "properties": {
        "frontendIPConfigurations": [
          {
            "name": "[parameters('geminiWebLBFE')]",
            "properties": {
              "publicIPAddress": {
                "id": "[parameters('spWebResourceId')]"
              }
            }
          }
        ],
        "backendAddressPools": [
          {
            "name": "[parameters('geminiWebLBBE')]"
          }
        ],
        "loadBalancingRules": [
          {
            "name": "[parameters('geminiWebLB')]",
            "properties": {
              "frontendIPConfiguration": {
                "id": "[parameters('geminiWebLBFEConfigID')]"
              },
              "backendAddressPool": {
                "id": "[parameters('geminiWebBEAddressPoolID')]"
              },
              "probe": {
                "id": "[parameters('geminiWebProbeID')]"
              },
              "protocol": "Tcp",
              "frontendPort": 80,
              "backendPort": 80,
              "enableFloatingIP": false
            }
          }
        ],
        "probes": [
          {
            "name": "[parameters('gemininiWebProbe')]",
            "properties": {
              "protocol": "Http",
              "port": 8088,
              "intervalInSeconds": 15,
              "numberOfProbes": 5,
              "requestPath": "/iisstart.htm"
            }
          }
        ]
      }
    },
    {
      "apiVersion": "2015-06-15",
      "name": "[parameters('sqlLBName')]",
      "type": "Microsoft.Network/loadBalancers",
      "location": "[parameters('location')]",
      "properties": {
        "frontendIPConfigurations": [
          {
            "name": "[parameters('sqlLBFE')]",
            "properties": {
              "privateIPAllocationMethod": "Static",
              "privateIPAddress": "[parameters('sqlLBIPAddress')]",
              "subnet": {
                "id": "[parameters('staticSubnetRef')]"
              }
            }
          }
        ],
        "backendAddressPools": [
          {
            "name": "[parameters('sqlLBBE')]"
          }
        ],
        "loadBalancingRules": [
          {
            "name": "SQLAlwaysOnEndPointListener",
            "properties": {
              "frontendIPConfiguration": {
                "id": "[parameters('sqllbFEConfigID')]"
              },
              "probe": {
                "id": "[parameters('sqllbProbeID')]"
              },
              "protocol": "Tcp",
              "frontendPort": 1433,
              "backendPort": 1433,
              "enableFloatingIP": true
            }
          }
        ],
        "probes": [
          {
            "name": "[parameters('SQLAOProbe')]",
            "properties": {
              "protocol": "Tcp",
              "port": 59999,
              "intervalInSeconds": 5,
              "numberOfProbes": 2
            }
          }
        ]
      }
    }
  ]
}